<?xml version="1.0" encoding="ASCII"?>
<ThingSpecification
    xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns="http://yetu.com/models/ThingSpecification"
    xsi:schemaLocation="http://yetu.com/models/ThingSpecification ../model/ThingSpecification.ecore">
 
    <capabilities
        id="SWITCHABLE">
      <properties
          name="on"
          writable="true"
          unit="BOOLEAN"/>
    </capabilities>
    <capabilities
        id="MEASUREMENT">
      <properties
          name="value"/>
    </capabilities>
    <capabilities
        id="LOCKABLE">
      <properties
          name="locked"
          writable="true"
          unit="BOOLEAN"/>
    </capabilities>
    <capabilities
        id="SETTABLE">
      <properties
          name="value"
          writable="true"/>
    </capabilities>
    <capabilities
        id="DETECTION">
      <properties
          name="detected"
          unit="BOOLEAN"/>
    </capabilities>
    <capabilities
        id="DIMMABLE">
      <properties
          name="dimmlevel"
          writable="true"
          unit="PERCENTAGE"/>
    </capabilities>
 
    <componentTypes
        capability="SWITCHABLE"
        id="SOCKET"/>
    <componentTypes
        capability="SWITCHABLE"
        id="LAMP"/>
    <componentTypes
        capability="SETTABLE"
        id="THERMOSTAT"/>
    <componentTypes
        capability="MEASUREMENT"
        id="SENSOR"/>
    <componentTypes
        capability="MEASUREMENT"
        id="METER"/>
    <componentTypes
        capability="LOCKABLE"
        id="LOCK"/>
    <componentTypes
        capability="DETECTION"
        id="ALARM"/>
    <componentTypes
        capability="MEASUREMENT"
        id="BATTERY"/>
 
    <units id="BOOLEAN"
        symbol=""
        valueType="BOOLEAN"/>
    <units id="NUMBER"
        valueType="NUMBER"/>
    <units id="WATT"
        symbol="W"
        valueType="NUMBER"/>
    <units id="PERCENTAGE"
        symbol="%"
        valueType="NUMBER"/>
 
</ThingSpecification>
