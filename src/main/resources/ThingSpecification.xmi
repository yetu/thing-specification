<?xml version="1.0" encoding="ASCII"?>
<ThingSpecification
    xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns="http://yetu.com/spec/abstraction"
    xsi:schemaLocation="http://yetu.com/spec/abstraction ../model/ThingSpecification.ecore">
  <capabilitySpecifications
      id="SWITCHABLE">
    <properties
        name="on"
        writable="true"
        unit="BOOLEAN"/>
  </capabilitySpecifications>
  <capabilitySpecifications
      id="COLORABLE">
    <properties
        name="color"
        writable="true"
        unit="HSBCOLOR"/>
  </capabilitySpecifications>
  <capabilitySpecifications
      id="MEASUREMENT">
    <properties
        name="measurement"/>
  </capabilitySpecifications>
  <capabilitySpecifications
      id="SETTABLE">
    <properties
        name="value"
        writable="true"/>
  </capabilitySpecifications>
  <capabilitySpecifications
      id="LOCKABLE">
    <properties
        name="locked"
        writable="true"
        unit="BOOLEAN"/>
  </capabilitySpecifications>
  <units id="BOOLEAN"
      valueType="BOOLEAN"/>
  <units id="HSBCOLOR"
      symbol=""
      valueType="STRING"/>
  <units id="WATTHOUR"
      symbol="Wh"
      valueType="NUMBER"/>
  <units id="WATT"
      symbol="W"
      valueType="NUMBER"/>
  <units id="ILLUMINANCE"
      symbol="lx"
      valueType="NUMBER"/>
  <units id="TEMPERATURE"
      symbol="&#xb0;C"
      valueType="NUMBER"/>
  <units id="PERCENTAGE"
      symbol="%"
      valueType="NUMBER"/>
  <units id="HUMIDITY"
      symbol="%"
      valueType="NUMBER"/>
  <units id="STRING"
      symbol=""
      valueType="STRING"/>
  <componentSpecifications
      type="SOCKET"
      capabilities="SWITCHABLE"/>
  <componentSpecifications
      type="LAMP"
      capabilities="SWITCHABLE"/>
  <componentSpecifications
      type="METER"
      capabilities="MEASUREMENT"/>
  <componentSpecifications
      type="SENSOR"
      capabilities="MEASUREMENT"/>
  <componentSpecifications
      type="BATTERY"/>
  <componentSpecifications
      type="THERMOSTAT"
      capabilities="SETTABLE"/>
  <componentSpecifications
      type="WEBSERVICE"
      capabilities="SETTABLE"/>
  <componentSpecifications
      type="LOCK"
      capabilities="LOCKABLE"/>
  <componentTypes
      id="METER"/>
  <componentTypes
      id="SOCKET"/>
  <componentTypes
      id="LAMP"/>
  <componentTypes
      id="SENSOR"/>
  <componentTypes
      id="BATTERY"/>
  <componentTypes
      id="THERMOSTAT"/>
  <componentTypes
      id="WEBSERVICE"/>
  <componentTypes
      id="LOCK"/>
</ThingSpecification>
