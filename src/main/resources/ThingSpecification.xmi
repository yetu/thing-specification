<?xml version="1.0" encoding="ASCII"?>
<ThingSpecification
    xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns="http://yetu.com/spec/abstraction"
    xsi:schemaLocation="http://yetu.com/spec/abstraction ../model/ThingSpecification.ecore">
  <capabilities
      id="SWITCHABLE">
    <properties
        name="on"
        writable="true"
        unit="BOOLEAN"/>
  </capabilities>
  <capabilities
      id="MEASUREMENT">
    <properties
        name="value"/>
  </capabilities>
  <capabilities
      id="LOCKABLE">
    <properties
        name="locked"
        writable="true"
        unit="BOOLEAN"/>
  </capabilities>
  <units id="WATT"
      symbol="W"
      valueType="NUMBER"/>
  <units id="BOOLEAN"
      symbol=""
      valueType="BOOLEAN"/>
  <components
      type="SOCKET"
      capabilities="SWITCHABLE"/>
  <components
      type="LAMP"
      capabilities="SWITCHABLE"/>
  <componentTypes
      id="SOCKET"/>
  <componentTypes
      id="LAMP"/>
</ThingSpecification>
